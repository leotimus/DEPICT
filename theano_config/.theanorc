[global]
cxx=/usr/bin/g++-5
cc=/usr/bin/gcc-5
device=cpu
optimizer_excluding=low_memory
allow_gc=False
floatX=float64
root=/usr/local/cuda
[dnn.conv]
algo_fwd = time_once
algo_bwd_data = time_once
algo_bwd_filter = time_once
[nvcc]
fastmath=True
[blas]
ldflags=-lmkl_rt
nvcc.flags=std=c++11 -D__CORRECT_ISO_CPP11_MATH_H_PROTO -D_FORCE_INLINES
